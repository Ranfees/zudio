<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Zudio Carousel</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/js/bootstrap.bundle.min.js"></script>
</head>
<style>
   
    .butn{
       
         height: 15px !important;
         width: 15px !important;
         border-radius: 50% !important;
    }
    .main-bdy{
        background-color: black;
        height: 90.5vh;
    }
  
</style>
<body>
    <%- include('navbar') %>
    <%- include('side-bar-with-btns') %>

    <div class="main-bdy">
        <div id="myCarousel" class="carousel slide carousel-fade" data-bs-ride="carousel">
            <!-- Indicators -->
             <div class="carousel-indicators d-flex align-content-end justify-content-end">
                <button  type="button" data-bs-target="#myCarousel" data-bs-slide-to="0" class="active butn" aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#myCarousel" data-bs-slide-to="1" class="butn" aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#myCarousel" data-bs-slide-to="2" class="butn" aria-label="Slide 3"></button>
                <button type="button" data-bs-target="#myCarousel" data-bs-slide-to="3" class="butn" aria-label="Slide 4"></button>
                
            </div>


            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="/women/AW_FS_web_banners-12_1950x.webp" class="d-block w-100">
                </div>
                <div class="carousel-item">
                    <img src="/women/AW_FS_web_banners-13_1950x.jpg" class="d-block w-100">
                </div>
                <div class="carousel-item">
                    <img src="/women/AW_FS_web_banners-14_1950x.jpg" class="d-block w-100">
                </div>
                 <div class="carousel-item">
                    <img src="/women/AW_FS_web_banners-15_1950x.jpg" class="d-block w-100">
                </div>
             
            </div>
        </div>
    </div>
</body>

</html>